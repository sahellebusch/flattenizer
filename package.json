{
  "name": "flattenizer",
  "description": "Small library for flattening and unflattening objects.",
  "version": "1.2.0-beta.0",
  "author": "Sean Hellebusch <sahellebusch@gmail.com>",
  "bugs": {
    "url": "https://github.com/sahellebusch/flattenizer/issues"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^22.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.0",
    "ts-jest": "^29.2.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/sahellebusch/flattenizer#readme",
  "keywords": [
    "flatten",
    "unflatten"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sahellebusch/flattenizer.git"
  },
  "scripts": {
    "build": "tsup",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "prepublishOnly": "npm run build",
    "release": "np",
    "start": "tsup --watch",
    "test": "jest"
  }
}
